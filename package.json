{
	"name": "obsidian-typst-pdf-export",
	"version": "1.3.0",
	"description": "Exports a note or collection of notes to a pretty Typst PDF.",
	"main": "main.js",
	"scripts": {
		"typecheck": "tsc --noEmit",
		"lint": "eslint src/ main.ts",
		"dev": "npm run typecheck && node esbuild.config.mjs",
		"build": "npm run typecheck && npm run lint && node esbuild.config.mjs production",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"package": "node scripts/package-plugin.js",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"pdf",
		"export",
		"typst",
		"obsidian-plugin",
		"typesetting"
	],
	"author": "Alexander Kucera",
	"license": "MIT",
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.34.0",
		"@types/node": "^20.19.24",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"@vitest/coverage-v8": "^4.0.6",
		"@vitest/ui": "^4.0.6",
		"archiver": "^7.0.1",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"globals": "^16.3.0",
		"happy-dom": "^20.0.10",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.43.0",
		"vitest": "^4.0.6"
	},
	"dependencies": {
		"gray-matter": "^4.0.3",
		"js-yaml": "^4.1.0",
		"pdf-to-img": "^4.5.0"
	}
}
